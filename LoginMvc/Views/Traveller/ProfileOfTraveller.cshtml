@model List<LoginMvc.Models.tripposts>

@{
    ViewBag.Title = "ProfileOfTraveller";
    Layout = "~/Views/Traveller/TravellerLayout.cshtml";
}


<div class="col-xl-7 col-lg-7 col-md-12">


    @using (Html.BeginForm("ProfileOfTraveller", "Traveller", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
                <form class="tm-edit-product-form">
                    <div class="input-group mb-3">
                        <label for="que" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">
                            que Name

                        </label>
                        <input id="que" required name="que" type="text" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7">
                    </div>


                    <div class="form-group">
                        <div class="col-md-12 text-center" style="margin-top:20px">
                            <input type="submit" value="Create" class="btn btn-default btn-lg btn-block" />
                        </div>
                    </div>
                </form>}
</div>
